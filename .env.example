# Release Dashboard Environment Variables
# Copy this to .env for local development, or set in AWS App Runner

# Jenkins Configuration
JENKINS_BASE_URL=https://your-jenkins.example.com
JENKINS_USERNAME=your-username
JENKINS_API_TOKEN=your-api-token

# AI Configuration (Gemini)
AI_PROVIDER=gemini
AI_API_KEY=your-gemini-api-key
AI_MODEL=gemini-2.5-flash

# Sentry Configuration
SENTRY_BASE_URL=https://sentry.io
SENTRY_AUTH_TOKEN=your-sentry-auth-token
SENTRY_ORG=your-organization

# Discord Configuration
DISCORD_ENABLED=true
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your-webhook-id/your-webhook-token

# App Store Connect (for iOS)
ASC_KEY_ID=your-key-id
ASC_ISSUER_ID=your-issuer-id
# For AWS: store the .p8 key content directly
ASC_KEY_CONTENT=-----BEGIN PRIVATE KEY-----\nYOUR_KEY_HERE\n-----END PRIVATE KEY-----

# Google Play (for Android)
# For AWS: store the JSON key content directly
GOOGLE_PLAY_KEY_CONTENT={"type":"service_account",...}

# Optional
REFRESH_INTERVAL=60000
BRANCH_HISTORY_DAYS=30
